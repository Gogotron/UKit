{
    "expo": {
        "name": "Ukit",
        "description": "Ukit, a companion app for student of University of Bordeaux",
        "slug": "Ukit",
        "privacy": "public",
        "githubUrl": "https://github.com/Jack3113/UKit",
        "sdkVersion": "30.0.0",
        "version": "3.2.1",
        "orientation": "portrait",
        "primaryColor": "#006F9F",
        "icon": "./assets/icons/app_1024_alpha.png",
        "splash": {
            "image": "./assets/icons/splash.png",
            "backgroundColor": "#009de0"
        },
        "packagerOpts": {
            "assetExts": [
                "ttf",
                "mp4"
            ]
        },
        "ios": {
            "icon": "./assets/icons/app_1024.png",
            "supportsTablet": true,
            "bundleIdentifier": "com.bordeaux.ukit"
        },
        "android": {
            "package": "com.bordeaux1.emplois",
            "permissions": [],
            "versionCode": 47
        },
        "androidStatusBar": {
            "barStyle": "light-content",
            "backgroundColor": "#009ee0"
        },
        "assetBundlePatterns": [
            "assets/icons/**"
        ],
        "updates": {
            "enabled": false
        }
    }
}
